{
  "name": "{{locales.plugin_name}}",
  "description": "Object Recognition",
  "author": "Colabeler",
  "version": "0.0.1",
  "identifier": "com.colabeler.cv.classification",
  "homepage_url": "http://www.colabeler.com",
  "manifest_version": 1,
  "icon":"src/assets/image/icon.svg",
  "group":"{{locales.group_name}}",
  "inputs":[
    {
      "name":"source",
      "label":"{{locales.inputs_source}}",
      "type":"folder.image"
    },
    {
      "name":"classify_value",
      "label":"{{locales.inputs_classify}}",
      "type":"string",
      "default":"{{locales.inputs_classify_default}}"
    },
    {
      "name":"classify_count",
      "label":"{{locales.inputs_count}}",
      "type":"int32",
      "default":"1"
    }
  ],
  "outputs":[
    {
      "name":"object",
      "type":"pascal-voc"
    }
  ],
  "main":"src/index.html",
  "default_locale":"en"
}