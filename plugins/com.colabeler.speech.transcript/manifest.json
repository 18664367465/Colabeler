{
  "name": "{{locales.plugin_name}}",
  "description": "Speech Recognition",
  "author": "Colabeler",
  "version": "0.0.1",
  "identifier": "com.colabeler.speech.transcript",
  "homepage_url": "http://www.colabeler.com",
  "manifest_version": 1,
  "icon":"src/assets/image/icon.svg",
  "group":"{{locales.group_name}}",
  "inputs":[
    {
      "name":"source",
      "label":"{{locales.inputs_source}}",
      "type":"folder.audio"
    }
  ],
  "outputs":[
    {
      "name":"transcript",
      "type":"string"
    }
  ],
  "main":"src/index.html",
  "default_locale":"en"
}